{
  "name": "swvl_task",
  "version": "0.0.1",
  "description": "swvl task",
  "author": "abdelrahman mysara",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "start": "nest start",
    "test": "cross-env NODE_ENV=test jest --forceExit --logHeapUsage --verbose --watchAll=false --maxWorkers=50%",
    "test:cov": "jest --coverage --forceExit --logHeapUsage --verbose --runInBand",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node --trace-warnings dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@firebase/app-types": "0.x",
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/mongoose": "^7.2.4",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.8.0",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "firebase-admin": "^9.11.0",
    "mongoose": "^5.12.14",
    "mongoose-aggregate-paginate-v2": "^1.0.42",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.6",
    "twilio": "^3.66.1"
  },
  "config": {
    "mongodbMemoryServer": {
      "arch": "x64"
    }
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/faker": "^5.5.6",
    "@types/jest": "26.0.10",
    "@types/node": "^13.9.1",
    "@types/supertest": "^2.0.8",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "faker": "^5.5.3",
    "jest": "26.4.2",
    "mongodb-memory-server": "^6.9.6",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.7.4"
  }
}
